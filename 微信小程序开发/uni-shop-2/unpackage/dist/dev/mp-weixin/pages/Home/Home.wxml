<view><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="true"><block wx:for="{{swiperlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><navigator class="swiper-item" url="{{'/subpkg/goods_detail/goods_detail?goods_id='+item.goods_id}}"><image src="{{item.image_src}}"></image></navigator></swiper-item></block></swiper><view class="nav-list"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navClickHandler',['$0'],[[['navList','',index]]]]]]]}}" class="nav-item" bindtap="__e"><image class="nav-img" src="{{item.image_src}}"></image></view></block></view><view class="floor-list"><block wx:for="{{floorList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="floor-item"><image class="floor-title" src="{{item.floor_title.image_src}}"></image><view class="floor-img-box"><view class="left-img-box"><image style="{{'width:'+(item.product_list[0].image_width+'rpx')+';'}}" mode="heightFix" src="{{item.product_list[0].image_src}}"></image></view><view class="right-img-box"></view></view></view></block></view></view>